{
    "name": "brandembassy/coding-standard",
    "description": "Brand Embassy Coding Standard",
    "license": "proprietary",
    "type": "phpcodesniffer-standard",
    "authors": [
        {
            "name": "Brand Embassy"
        }
    ],
    "autoload": {
        "psr-4": {
            "BrandEmbassyCodingStandard\\": "src/BrandEmbassyCodingStandard"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\BrandEmbassyCodingStandard\\": "tests/BrandEmbassyCodingStandard"
        }
    },
    "require": {
        "squizlabs/php_codesniffer": "^3.3",
        "slevomat/coding-standard": "^5.0",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.4",
        "phpstan/phpstan": "^0.11",
        "phpstan/phpstan-strict-rules": "^0.11",
        "phpstan/phpstan-nette": "^0.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "mockery/mockery": "^1.2"
    },
    "scripts": {
        "phpcs": "vendor/bin/phpcs --standard=BrandEmbassyCodingStandard src tests",
        "phpcbf": "vendor/bin/phpcbf --standard=BrandEmbassyCodingStandard src tests",
        "phpunit": "vendor/bin/phpunit",
        "phpstan": "vendor/bin/phpstan analyze -c phpstan.neon src"
    }
}
